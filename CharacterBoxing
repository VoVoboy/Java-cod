package AutoBoxingSwitch;

/**
 * Created by RogueBoy on 08.12.2016.
 */
public class AutoBoxingSwitch {
    public static void main(String args[]){
        Boolean b = true;

        if(b) System.out.println("b is true");

        Character ch = 'x';
        char ch2 = ch;

        System.out.println("ch2 is  " + ch2);
    }
}
